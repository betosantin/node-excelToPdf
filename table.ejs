<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Table</title>
</head>
<body>

    <table>
        <head>
            <% header.forEach(col => { %>
                <td><%= col %></td>
            <% }) %>
        </head>

        <tbody>
            <% rows.forEach(row => { %>
                <tr><% row.forEach(e => { %>
                        <td><%= e %></td>
                    <% }) %>
                </tr>
            <% }) %>

        </tbody>
    </table>
    
</body>
</html>